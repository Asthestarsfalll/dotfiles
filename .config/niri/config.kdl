    window-close {
        custom-shader r#"
            //glsl
            // 经典电视关机特效：椭圆形光斑和线条收缩，颜色为白色
            
            // 缓动函数 - 使用 easeInOutCubic，使收缩速度逐渐变慢（越接近中心越慢）
            float easeInOutCubic(float x) {
                return x < 0.5 
                    ? 4.0 * x * x * x 
                    : 1.0 - pow(-2.0 * x + 2.0, 3.0) / 2.0;
            }

            // 主函数：实现电视关机特效
            vec4 close_color(vec3 coords_geo, vec3 size_geo) {
                vec2 uv = coords_geo.st;
                
                float progress = niri_progress;
                float shrinkAmount = easeInOutCubic(progress);
                
                vec2 center = vec2(0.5, 0.5);
                
                // 计算相对于中心的坐标
                vec2 d = uv - center;
                
                // 水平光束效果：创建一个非常扁平的水平椭圆光斑
                float initialRadiusX = 0.2; // 水平方向较大，形成水平光束
                float initialRadiusY = 0.06; // 垂直方向较小，但不要太极端
                float spotRadiusX = initialRadiusX * (1.0 - shrinkAmount);
                float spotRadiusY = initialRadiusY * (1.0 - shrinkAmount);
                
                // 使用椭圆距离计算
                vec2 normalizedD = d / vec2(spotRadiusX, spotRadiusY);
                float ellipseDist = length(normalizedD);
                
                // 中心光斑：平滑的边缘渐变
                float spotFadeStart = 0.3;
                float spotFadeEnd = 1.0;
                float spotMask = 1.0 - smoothstep(spotFadeStart, spotFadeEnd, ellipseDist);
                
                // 增强中心亮度：中心区域非常亮，向边缘逐渐变暗
                // 使用更陡的衰减曲线，使中心更突出
                float centerBrightness = 1.0 - smoothstep(0.0, 0.4, ellipseDist);
                // 将中心亮度叠加到光斑上，确保中心始终保持最亮
                spotMask = max(spotMask, centerBrightness * 2.5);
                
                // 添加光扩散效果：创建一个柔和的轻微光晕
                float glowRadiusX = spotRadiusX * 1.1;
                float glowRadiusY = spotRadiusY * 1.1;
                vec2 normalizedGlowD = d / vec2(glowRadiusX, glowRadiusY);
                float glowDist = length(normalizedGlowD);
                float glowMask = 1.0 - smoothstep(0.7, 1.1, glowDist);
                glowMask *= 0.4; // 光晕强度，保持轻微
                
                // 将光晕叠加到光斑上
                spotMask = max(spotMask, glowMask);
                
                // 计算垂直线条遮罩（垂直方向收缩）- 主要关机效果
                float distFromCenterY = abs(uv.y - 0.5);
                float visibleRangeY = (1.0 - shrinkAmount) * 0.5;
                
                // 线条宽度：中心窄，边缘宽
                float centerLineWidth = 0.012;
                float edgeLineWidth = 0.04;
                // 根据到中心的水平距离计算线条宽度
                float distFromCenterX = abs(uv.x - 0.5);
                float lineWidth = mix(centerLineWidth, edgeLineWidth, distFromCenterX * 2.0);
                
                // 垂直线条遮罩：平滑的过渡
                float lineDist = distFromCenterY / (visibleRangeY + 0.001);
                float lineFadeStart = 0.0;
                float lineFadeEnd = lineWidth * 2.0;
                float lineMask = 1.0 - smoothstep(lineFadeStart, lineFadeEnd, lineDist);
                
                // 根据到中心的水平距离调整线条亮度：中心最亮，边缘最暗
                float brightness = 1.0 - smoothstep(0.0, 0.45, distFromCenterX);
                lineMask *= brightness;
                // 增强线条亮度
                lineMask *= 1.8;
                
                // 当收缩接近完成时，强制垂直线条消失，避免残留细线
                float lineFadeOut = 1.0 - smoothstep(0.85, 0.98, shrinkAmount);
                lineMask *= lineFadeOut;
                
                // 在光斑内部添加水平纹理线条
                float horizontalLineFreq = 20.0; // 水平线条的频率
                float horizontalLinePattern = sin(uv.y * horizontalLineFreq * 3.14159);
                float horizontalLines = smoothstep(0.95, 1.0, abs(horizontalLinePattern));
                horizontalLines *= spotMask * 0.3; // 只在光斑内显示，较淡
                
                // 当收缩接近完成时，强制水平纹理线条消失
                horizontalLines *= lineFadeOut;
                
                // 当收缩接近完成时，强制光斑消失，避免残留细线
                float spotFadeOut = 1.0 - smoothstep(0.85, 0.98, shrinkAmount);
                spotMask *= spotFadeOut;
                
                // 组合光斑、垂直线条和水平纹理
                float finalMask = max(spotMask, lineMask);
                finalMask = max(finalMask, horizontalLines);
                
                // 计算到中心的距离，用于调整颜色和亮度
                float distFromCenter = length(d);
                float maxDist = length(vec2(0.5, 0.5));
                float centerFactor = 1.0 - smoothstep(0.0, maxDist * 0.6, distFromCenter);
                
                // 偏蓝的白色：中心更蓝更亮，边缘偏白
                // RGB: 中心 (0.85, 0.92, 1.0) 到边缘 (0.95, 0.98, 1.0)
                vec3 blueTint = mix(
                    vec3(0.95, 0.98, 1.0),  // 边缘：偏白的蓝色
                    vec3(0.85, 0.92, 1.0)   // 中心：更蓝更亮
                , centerFactor);
                
                // 增强整体亮度，中心区域更亮
                float brightnessMultiplier = 1.0 + centerFactor * 1.5;
                finalMask *= brightnessMultiplier;
                
                // 使用偏蓝的白色，并应用亮度
                vec4 lightColor = vec4(blueTint * finalMask, finalMask);
                
                return lightColor;
            }"#
    }
    window-resize {
        custom-shader r#"
            //glsl
            // Example: fill the current geometry with a solid vertical gradient.
            vec4 solid_gradient(vec3 coords_curr_geo, vec3 size_curr_geo) {
                vec3 coords = coords_curr_geo;
                vec4 color = vec4(0.0);

                // Paint only the area inside the current geometry.
                if (0.0 <= coords.x && coords.x <= 1.0
                        && 0.0 <= coords.y && coords.y <= 1.0)
                {
                    vec4 from = vec4(1.0, 0.0, 0.0, 1.0);
                    vec4 to = vec4(0.0, 1.0, 0.0, 1.0);
                    color = mix(from, to, coords.y);
                }

                return color;
            }

            // Example: crossfade between previous and next texture, stretched to the
            // current geometry.
            vec4 crossfade(vec3 coords_curr_geo, vec3 size_curr_geo) {
                // Convert coordinates into the texture space for sampling.
                vec3 coords_tex_prev = niri_geo_to_tex_prev * coords_curr_geo;
                vec4 color_prev = texture2D(niri_tex_prev, coords_tex_prev.st);

                // Convert coordinates into the texture space for sampling.
                vec3 coords_tex_next = niri_geo_to_tex_next * coords_curr_geo;
                vec4 color_next = texture2D(niri_tex_next, coords_tex_next.st);

                vec4 color = mix(color_prev, color_next, niri_clamped_progress);
                return color;
            }

            // Example: next texture, stretched to the current geometry.
            vec4 stretch_next(vec3 coords_curr_geo, vec3 size_curr_geo) {
                vec3 coords_tex_next = niri_geo_to_tex_next * coords_curr_geo;
                vec4 color = texture2D(niri_tex_next, coords_tex_next.st);
                return color;
            }

            // Example: next texture, stretched to the current geometry if smaller, and
            // cropped if bigger.
            vec4 stretch_or_crop_next(vec3 coords_curr_geo, vec3 size_curr_geo) {
                vec3 coords_next_geo = niri_curr_geo_to_next_geo * coords_curr_geo;

                vec3 coords_stretch = niri_geo_to_tex_next * coords_curr_geo;
                vec3 coords_crop = niri_geo_to_tex_next * coords_next_geo;

                // We can crop if the current window size is smaller than the next window
                // size. One way to tell is by comparing to 1.0 the X and Y scaling
                // coefficients in the current-to-next transformation matrix.
                bool can_crop_by_x = niri_curr_geo_to_next_geo[0][0] <= 1.0;
                bool can_crop_by_y = niri_curr_geo_to_next_geo[1][1] <= 1.0;

                vec3 coords = coords_stretch;
                if (can_crop_by_x)
                    coords.x = coords_crop.x;
                if (can_crop_by_y)
                    coords.y = coords_crop.y;

                vec4 color = texture2D(niri_tex_next, coords.st);

                // However, when we crop, we also want to crop out anything outside the
                // current geometry. This is because the area of the shader is unspecified
                // and usually bigger than the current geometry, so if we don't fill pixels
                // outside with transparency, the texture will leak out.
                //
                // When stretching, this is not an issue because the area outside will
                // correspond to client-side decoration shadows, which are already supposed
                // to be outside.
                if (can_crop_by_x && (coords_curr_geo.x < 0.0 || 1.0 < coords_curr_geo.x))
                    color = vec4(0.0);
                if (can_crop_by_y && (coords_curr_geo.y < 0.0 || 1.0 < coords_curr_geo.y))
                    color = vec4(0.0);

                return color;
            }

            // Example: cropped next texture if it's bigger than the current geometry, and
            // crossfade between previous and next texture otherwise.
            vec4 crossfade_or_crop_next(vec3 coords_curr_geo, vec3 size_curr_geo) {
                vec3 coords_next_geo = niri_curr_geo_to_next_geo * coords_curr_geo;
                vec3 coords_prev_geo = niri_curr_geo_to_prev_geo * coords_curr_geo;

                vec3 coords_crop = niri_geo_to_tex_next * coords_next_geo;
                vec3 coords_stretch = niri_geo_to_tex_next * coords_curr_geo;
                vec3 coords_stretch_prev = niri_geo_to_tex_prev * coords_curr_geo;

                // We can crop if the current window size is smaller than the next window
                // size. One way to tell is by comparing to 1.0 the X and Y scaling
                // coefficients in the current-to-next transformation matrix.
                bool can_crop_by_x = niri_curr_geo_to_next_geo[0][0] <= 1.0;
                bool can_crop_by_y = niri_curr_geo_to_next_geo[1][1] <= 1.0;
                bool crop = can_crop_by_x && can_crop_by_y;

                vec4 color;

                if (crop) {
                    // However, when we crop, we also want to crop out anything outside the
                    // current geometry. This is because the area of the shader is unspecified
                    // and usually bigger than the current geometry, so if we don't fill pixels
                    // outside with transparency, the texture will leak out.
                    //
                    // When crossfading, this is not an issue because the area outside will
                    // correspond to client-side decoration shadows, which are already supposed
                    // to be outside.
                    if (coords_curr_geo.x < 0.0 || 1.0 < coords_curr_geo.x ||
                            coords_curr_geo.y < 0.0 || 1.0 < coords_curr_geo.y) {
                        color = vec4(0.0);
                    } else {
                        color = texture2D(niri_tex_next, coords_crop.st);
                    }
                } else {
                    // If we can't crop, then crossfade.
                    color = texture2D(niri_tex_next, coords_stretch.st);
                    vec4 color_prev = texture2D(niri_tex_prev, coords_stretch_prev.st);
                    color = mix(color_prev, color, niri_clamped_progress);
                }

                return color;
            }

            // This is the function that you must define.
            vec4 resize_color(vec3 coords_curr_geo, vec3 size_curr_geo) {
                // You can pick one of the example functions or write your own.
                return crossfade_or_crop_next(coords_curr_geo, size_curr_geo);
            }"#
    }
